<h1 id="pageHeader">
  You are now viewing article:
  {{articleObj.title}}
</h1>
<div id="infoBody">
  <div id="editDelete">
    <div>
      <a href="/articles/{{articleObj.url}}/edit">
        <button>
          edit
        </button>
      </a>
    </div>
    <div>
      <a href="/articles/{{articleObj.url}}/delete">
        <button>
          delete
        </button>
      </a>
    </div>
  </div>
  <div id="listOfItems">
    Article #:
    {{articleObj.id}}
    <p>
      Author:
      {{articleObj.author}}
    </p>
    <p>
      Body:
      {{articleObj.body}}
    </p>
  </div>
  <div>
    <a href="/articles">
      <button>
        Back
      </button>
    </a>
  </div>
</div>